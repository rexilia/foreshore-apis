meta {
  name: Get external auth token
  type: http
  seq: 15
}

post {
  url: {{api-url}}/partners/{{partner-id}}/token
  body: json
  auth: none
}

headers {
  Content-Type: application/json
  API-KEY: {{api-key}}
  API-SECRET: {{api-secret}}
  MERCHANT-ID: {{merchant-id}}
}

body:json {
  {
  	"providerCertificate":"{{ _.certificate }}",
  	"providerId":"{{ _.providerId }}",
  	"externalUserId":"newTokenWithCreate"
  }
  
}